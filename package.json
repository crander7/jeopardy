{
  "name": "jeopardy",
  "description": "This is a jeopardy app for custom jeopardy games",
  "author": "Craig Andersen <craig.andersen@zefr.com>",
  "license": "MIT",
  "version": "0.0.0",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "repository": "https://github.com/crander7/jeopardy.git",
  "scripts": {
    "build-prod": "better-npm-run build-prod",
    "build-dev": "better-npm-run build-dev",
    "start-prod": "better-npm-run start-prod",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "npm run test:coverage && npm run test:lint",
    "test:coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha",
    "test:lint": "eslint -c .eslintrc.json src",
    "test:watch": "mocha --watch --reporter min",
    "docs": "./node_modules/esdoc/out/src/ESDocCLI.js -c esdoc.json"
  },
  "betterScripts": {
    "start-prod": {
      "command": "pm2 start ./bin/server.js --name jeopardy",
      "env": {
        "NODE_ENV": "prod",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build-prod": {
      "command": "webpack --progress --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "prod"
      }
    },
    "build-dev": {
      "command": "webpack --progress --colors --display-error-details --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "dev",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "^0.0.15",
    "body-parser": "^1.17.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "express": "^4.15.2",
    "express-session": "^1.12.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "hoist-non-react-statics": "^1.2.0",
    "http-proxy": "^1.16.2",
    "invariant": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.3.0",
    "less": "^2.5.3",
    "less-loader": "^4.0.3",
    "lru-memoize": "^1.0.2",
    "map-props": "^1.0.0",
    "material-ui": "^0.17.4",
    "node-sass": "^4.5.2",
    "pg": "^6.1.5",
    "piping": "^1.0.0-rc.4",
    "postcss-loader": "^1.3.3",
    "pretty-error": "^2.1.0",
    "prop-types": "^15.5.7",
    "q": "^1.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.12.3",
    "react-helmet": "^5.0.3",
    "react-inline-css": "^2.3.0",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-tooltip": "^3.2.10",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-form": "^6.6.3",
    "sass-loader": "^6.0.3",
    "serialize-javascript": "^2.1.1",
    "serve-favicon": "^2.4.2",
    "style-loader": "^0.16.1",
    "superagent": "^3.5.2",
